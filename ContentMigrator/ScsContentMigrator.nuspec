<?xml version="1.0"?>
<package >
  <metadata>
    <id>SitecoreSidekickContentMigrator</id>
    <version>$version$</version>
    <title>Sitecore Sidekick Content Migrator</title>
    <authors>Jeff Darchuk</authors>
    <owners>Jeff Darchuk</owners>
	<licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
	<projectUrl>https://github.com/JeffDarchuk/SitecoreSidekick</projectUrl>

    <requireLicenseAcceptance>false</requireLicenseAcceptance>
	<description>Easily move production Sitecore content to pre-production environments with this Sidekick module.</description>
	<copyright>Copyright 2017 Jeff Darchuk</copyright>
	<tags>sitecore</tags>
	<dependencies>
		<dependency id="SitecoreSidekickFoundation" version="$version$" />
		<dependency id="Rainbow.Core" version="1.4.2" />
		<dependency id="Rainbow.Storage.Sc" version="1.4.2" />
		<dependency id="Rainbow.Storage.Yaml" version="1.4.2" />
		<dependency id="htmldiff.net" version="1.3" />
		<dependency id="MicroCHAP" version="1.2.2.2" />
	</dependencies>
  </metadata>
  <files>
		<file src="..\ContentMigrator\Standard Config Files\*.config" target="content\App_Config\Include\Sidekick" />
		<file src="..\ContentMigrator\Standard Config Files\*.example" target="content\App_Config\Include\Sidekick" />
	</files>
</package>